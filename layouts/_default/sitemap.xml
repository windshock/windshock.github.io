{{- printf "<?xml version=\"1.0\" encoding=\"utf-8\"?>\n" | safeHTML -}}
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{{- range .Sites }}
  {{- range .RegularPages }}
    <url>
      <loc>{{ .Permalink }}</loc>
      <lastmod>{{ .Lastmod.Format "2006-01-02T15:04:05Z07:00" }}</lastmod>
    </url>
  {{- end }}
{{- end }}
</urlset>
